{
    "name": "@driving_school/application",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev:serve": "@php artisan serve --host='0.0.0.0'",
        "development": "mix",
        "watch": "mix watch",
        "dev": "yarn watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "postinstall": "composer install",
        "test": "php artisan test --parallel",
        "lint": "prettier --write \"**/*.{ts,tsx,md,json,vue,scss}\" --loglevel=error",
        "lint:php": "composer clean-the-code",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "dependencies": {
        "@fullcalendar/core": "^5.11.3",
        "@fullcalendar/daygrid": "^5.11.3",
        "@fullcalendar/interaction": "^5.11.3",
        "@fullcalendar/list": "^5.11.3",
        "@fullcalendar/timegrid": "^5.11.3",
        "@fullcalendar/vue3": "^5.11.2",
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-vue3": "^0.6.0",
        "@inertiajs/progress": "^0.2.7",
        "@popperjs/core": "^2.11.6",
        "@quasar/extras": "^1.15.5",
        "@types/lodash": "^4.14.184",
        "@types/node": "^18.11.9",
        "@vuepic/vue-datepicker": "^3.5.0",
        "checker": "^0.5.2",
        "dotenv": "^16.0.1",
        "dynamic-import-polyfill": "^0.1.1",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.38",
        "pm2": "^5.2.0",
        "prettier": "^2.5.1",
        "quasar": "^2.10.0",
        "toastr": "^2.1.4",
        "turbo": "^1.2.16",
        "vue": "^3.2.36"
    },
    "devDependencies": {
        "@vuedx/typescript-plugin-vue": "^0.7.5",
        "axios": "^0.25",
        "browser-sync": "^2.27.10",
        "browser-sync-webpack-plugin": "^2.3.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "resolve-url-loader": "^5.0.0",
        "sass": "1.32.12",
        "sass-loader": "^11.0.1",
        "ts-loader": "^9.4.1",
        "typescript": "^4.7.4",
        "vue-loader": "^16.2.0"
    }
}
